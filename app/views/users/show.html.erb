<h1 style="text-align: center; color:white"class="makeCenter">User initial information</h1>
<table style="width:100%">
  <tr>
    <th>First Name</th>
    <th>Last Name</th> 
    <th>Age</th>
    <th>Gender</th>
    <th>Height</th>
    <th>Weight</th>
    <th>Blood Type</th>
    <th>Subscription Code</th>
    <th>Address</th>
  </tr>
  <tr>
    <td><%= @user.firstName %></td>
    <td><%= @user.lastName %></td> 
    <td><%= @user.age %></td>
    <td><%= @user.gender %></td>
    <td><%= @user.height %></td>
    <td><%= @user.weight %></td>
    <td><%= @user.bloodType %></td>
    <td><%= @user.subscriptionId %></td>
    <td><%= @user.address %></td>
  </tr>
</table>


